package com.TechStalWarts.pageObjects;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

import com.TechStalWarts.Reports.ExtentLogger;



public class BasePage {
	public  WebDriver driver;
	public BasePage( WebDriver driver) {
		this.driver=driver;

	}
	
	
	
	

	protected void sendKeys(By by,String value ,String elementName) {
			driver.findElement(by).sendKeys(value);
			ExtentLogger.pass(element +" is sent");
	}

	protected void clicK(By by,String elementName) {
		driver.findElement(by).click();
		ExtentLogger.pass(elementName +" is Clicked");
	}

}
